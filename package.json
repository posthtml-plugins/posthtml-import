{
  "name": "posthtml-import",
  "version": "0.8.1",
  "description": "Import for PostHTML",
  "main": "index.js",
  "engines": {"npm": ">=3", "node": ">=6"},
  "scripts": {
    "clean": "echo '=> Cleaning' && rm -rf .nyc_output coverage test/node_modules",
    "pretest": "echo '=> Linting' && standard",
    "test": "echo '=> Testing' && npm run clean && nyc ava 'test/*.test.js'",
    "start": "echo '=> Starting' && sudo npm test"
  },
  "ava": {
    "babel": "inherit"
  },
  "nyc": {
    "all": true,
    "require": "babel-register",
    "include": ["lib", "test"],
    "extension": [".js"]
  },
  "standard": {
    "ignore": []
  },
  "babel": {
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    },
    "presets": ["es2015"]
  },
  "dependencies": {
    "posthtml-parser": "^0.1.3"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-core": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "coveralls": "^2.11.9",
    "nyc": "^6.4.4",
    "posthtml": "^0.8.7",
    "standard": "^7.1.2"
  },
  "keywords": [
    "HTML",
    "PostHTML",
    "PostHTML Import"
  ],
  "author": {
    "name": "Michael Ciniawky",
    "email": "michael.ciniawsky@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/posthtml/posthtml-import"
  },
  "bugs": {
    "url": "https://github.com/posthtml/posthtml-import/issues"
  },
  "homepage": "https://github.com/posthtml/posthtml-import#readme",
  "license": "MIT"
}
